<Window x:Class="SaveGameBackupTool.About"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SaveGameBackupTool"
        mc:Ignorable="d"
        Title="About" Height="350" Width="400" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <Label x:Name="labelAppName" Content="SaveGame Backup Tool" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Label x:Name="labelVersion" Content="Version:" HorizontalAlignment="Left" Margin="42,32,0,0" VerticalAlignment="Top" FontSize="10"/>
        <Label x:Name="labelVersionNumber" Content="labelVersionNumber" HorizontalAlignment="Left" Margin="87,32,0,0" VerticalAlignment="Top" FontSize="10" RenderTransformOrigin="1.651,0.56"/>
        <Button x:Name="buttonClose" Content="Close" Margin="0,0,10,10" Click="buttonClose_Click" HorizontalAlignment="Right" Width="75" Height="20" VerticalAlignment="Bottom"/>
        <TextBox x:Name="textBoxInfo" TextWrapping="Wrap" Text="textBoxInfo" Margin="10,60,10,35" IsReadOnly="True" FontSize="10"/>

    </Grid>
</Window>
